<div class="contents row">
  <h2 class="hdg">投稿一覧</h2>
  <ul class="user-tabs">
    <li><p class="link"><%= link_to("/users/#{@user.id}/likes") do %>いいね一覧へ<% end %></p></li>
  </ul>
    <% @article.each do |article| %>
    <article class="box-01">
      <% if article.image != "" %>
        <div class="image"><%= image_tag article.image %></div>
      <% end %>
      <div class="inner">
        <p class="developer">開発者：<%= current_user.developer %></p>
        <p class="title">サービス名：<%= article.title %></p>
        <p class="content">説明：<%= article.content %></p>
        <p class="lang">言語：<%= article.lang %></p>
        <p class="url">URL：<a href="<%= article.url %>" target="_blank"><%= article.url %></a></p>
        <div class="more">
          <ul class="more_list">
            <li><%= link_to '削除', "/articles/#{article.id}", method: :delete %></li>
            <li><%= link_to '編集', "/articles/#{article.id}/edit", method: :get %></li>
          </ul>
        </div>
      </div>
    </article>
  <% end %>
</div>